{
  "compilerOptions": {
  "target": "ES2020",
  "module": "Node16",
  "strict": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "noImplicitAny": true,
  "forceConsistentCasingInFileNames": true,
  "moduleResolution": "Node16",
  "rootDir": "./src",
  "outDir": "./dist",
},
"exclude": ["node_modules"],
"include": ["src/**/*.ts"]

}